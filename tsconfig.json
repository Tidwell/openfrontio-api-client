{
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*"]
}
