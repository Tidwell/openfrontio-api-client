{
  "name": "openfrontio-api-client",
  "type": "module",
  "scripts": {
    "build": "npx pkgroll",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:write": "WRITE_API_RESPONSES=true npm run test:e2e",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "docs": "npx typedoc src/index.ts",
    "prepare": "husky"
  },
  "engine": "24.xx.xx",
  "version": "1.1.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "husky": "^9.1.7",
    "openfront-client": "github:openfrontio/OpenFrontIO",
    "pkgroll": "^2.21.4",
    "prettier": "^3.3.3",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
